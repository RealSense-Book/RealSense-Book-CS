<Window x:Class="RealSenseSample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Hand Image" SizeToContent="WidthAndHeight"
        Loaded="Window_Loaded" Unloaded="Window_Unloaded"
        >
    <Grid>
        <Image x:Name="ImageHand1" Width="640" Height="480" HorizontalAlignment="Left"/>
        <Image x:Name="ImageHand2" Width="640" Height="480" HorizontalAlignment="Left"
               Margin="640,0,0,0"/>
    </Grid>
</Window>

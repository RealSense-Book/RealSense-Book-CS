<Window x:Class="RealSenseSample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="480" Width="640"
        Loaded="Window_Loaded"
        Unloaded="Window_Unloaded"
        KeyDown="OnKeyDownHandler"
        >
    <Grid>
        <Image x:Name="ImageColor" />
        <Canvas x:Name="CanvasPoint" >
        </Canvas>
        <Canvas x:Name="CanvasForRect">
            <Label Content="Regist:Key R push" Height="40" Canvas.Left="10" Canvas.Top="50" Width="200" FontSize="18" Foreground="Red"/>
            <Label Content="UnRegist:Key U push" Height="40" Canvas.Left="10" Canvas.Top="100" Width="200" FontSize="18" Foreground="Red"/>
            <TextBox x:Name="noticeInfo" Height="47" Canvas.Left="10" TextWrapping="Wrap" Text="NoticeInfo" Canvas.Top="145" Width="200" FontSize="24"/>
        </Canvas>
    </Grid>
</Window>
